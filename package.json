{
  "name": "fingerprintjs2",
  "version": "2.1.1",
  "description": "Modern & flexible browser fingerprinting library",
  "repository": {
    "type": "git",
    "url": "https://github.com/Valve/fingerprintjs2.git"
  },
  "keywords": [
    "fraud",
    "fraud-detection",
    "fraud-prevention",
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "privacy"
  ],
  "author": "Valentin Vasilyev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Valve/fingerprintjs2/issues"
  },
  "homepage": "https://github.com/Valve/fingerprintjs2",
  "main": "src/index.js",
  "browser": "dist/fingerprint2.min.js",
  "files": [
    "dist",
    "flash"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "jasmine-expect": "^4.0.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-matchers": "^4.0.1",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "standard": "^10.0.3"
  },
  "scripts": {
    "build": "rollup --config",
    "pretest": "yarn build",
    "test": "karma start --single-run --browsers ChromeHeadless tests/karma.conf.js",
    "test:debug": "karma start --single-run=false --browsers Chrome tests/karma.conf.js",
    "gulp": "gulp",
    "lint": "standard --fix"
  },
  "standard": {
    "ignore": [
      "specs/lib",
      "specs/phantomjs-testrunner.js"
    ]
  }
